    char=char+1
    if(i==' '):
        space=space+1
    elif(i.isupper()):
        upper=upper+1
    elif(i.islower()):
        lower=lower+1
    elif(i.isdigit()):
        num=num+1
    else:
        special=special+1
        sp=sp+i
